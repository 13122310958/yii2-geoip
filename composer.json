{
  "name": "lysenkobv/yii2-geoip",
  "description": "Yii 2 GeoIP extension. Returns country, city, lat, lng of current or specified IP (uses MaxMind's GeoIP2 databases)",
  "keywords": ["geoip", "geolocation", "maxmind", "yii2"],
  "type": "yii2-extension",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Bohdan Lysenko",
      "email": "lysenkobv@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.0",
    "maxmind-db/reader": "~1.0",
    "maxmind-mmdb/city": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "maxmind-mmdb/city",
        "version": "1.0.0",
        "dist": {
          "url": "http://geolite.maxmind.com/download/geoip/database/GeoLite2-City.mmdb.gz",
          "type": "gzip"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "lysenkobv\\GeoIP\\": "src/"
    }
  }
}
